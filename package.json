{
  "name": "realistic-liquid-glass",
  "version": "0.2.0-beta.1",
  "description": "iOS 26 like liquid glass React component",
  "keywords": [
    "react",
    "liquid-glass",
    "ios",
    "ui",
    "component"
  ],
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "license": "MIT",
  "author": "reekystive",
  "sideEffects": [
    "**/*.css"
  ],
  "type": "module",
  "imports": {
    "#src/*": "./src/*"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./liquid-glass": {
      "types": "./dist/liquid-glass.d.ts",
      "default": "./dist/liquid-glass.js"
    },
    "./styles.css": {
      "default": "./dist/styles.css"
    },
    "./package.json": {
      "default": "./package.json"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc --build tsconfig.json && vite build",
    "dev": "storybook dev",
    "lint:eslint": "eslint . --cache --cache-strategy content --cache-location ./node_modules/.cache/eslint/eslintcache.json --max-warnings 0 --report-unused-disable-directives",
    "lint:tsc": "tsc --build tsconfig.json",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:update": "vitest run --update",
    "test:watch": "vitest --watch",
    "prepare": "pnpm run build"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "tailwind-merge": "^3.3.0"
  },
  "devDependencies": {
    "@cspell/eslint-plugin": "^9.0.2",
    "@eslint/js": "^9.28.0",
    "@microsoft/api-extractor": "^7.52.8",
    "@storybook/builder-vite": "^9.0.8",
    "@storybook/react-vite": "^9.0.8",
    "@tailwindcss/vite": "^4.1.8",
    "@types/eslint": "^9.6.1",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/node": "^24.0.0",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react-swc": "^3.10.2",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-storybook": "^9.0.7",
    "globals": "^16.2.0",
    "prettier": "^3.5.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prettier-plugin-tailwindcss": "^0.6.12",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "storybook": "^9.0.8",
    "tailwindcss": "^4.1.8",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.34.0",
    "unplugin-dts": "1.0.0-beta.0",
    "vite": "^6.3.5",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.3"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "packageManager": "pnpm@10.11.0"
}
